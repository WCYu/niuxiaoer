<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include layout="@layout/include_header_layout" />
    <ImageView
        android:layout_width="205px"
        android:layout_height="205px"
        android:layout_marginTop="138px"
        android:layout_gravity="center"
        android:src="@mipmap/tixian"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="提现成功"
        android:layout_marginTop="28px"
        android:textColor="@color/colorOrange"
        android:textSize="@dimen/text_size_l"
        android:gravity="center"/>
    <Button
        android:id="@+id/btn_tixian_fanhui"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:layout_marginTop="62dp"
        android:background="@drawable/selector_btn_sub"
        android:text="返回"
        android:textColor="@color/colorWhite"
        android:layout_marginStart="48px"
        android:layout_marginEnd="48px"
        android:textSize="34px"
        android:layout_below="@+id/rl_forget_password"
        android:layout_alignParentStart="true" />


    </LinearLayout>

